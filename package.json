{
    "name": "mini-juice",
    "version": "0.2.2",
    "description": "Now you can",
    "author": "denismart",
    "license": "MIT",
    "repository": "denismart/mini-juice",
    "scripts": {
        "build": "yarn test:lint && yarn test:jest && rm -rf ./dist && export BABEL_ENV=rollup && rollup --config",
        "build:dev": "rm -rf ./dist && export BABEL_ENV=rollup && rollup --config",
        "publish-it": "yarn build && cp package.json ./dist && cp README.md ./dist && git add -f ./dist/package.json && yarn publish dist && cp ./dist/package.json . && git rm --cached ./dist/package.json",
        "test:lint": "eslint .",
        "test:jest": "jest --passWithNoTests"
    },
    "peerDependencies": {
        "react": "^16.0.0",
        "react-ga": "^3.3.0"
    },
    "devDependencies": {
        "@babel/plugin-external-helpers": "^7.12.1",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@betit/rollup-plugin-rename-extensions": "^0.1.0",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-replace": "^2.3.4",
        "cross-env": "^7.0.2",
        "eslint": "^5.16.0 || ^6.8.0 || ^7.2.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4 || ^3 || ^2.3.0 || ^1.7.0",
        "jest": "^26.6.3",
        "prettier": "^2.0.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-scripts": "^3.4.1",
        "rollup": "^2.35.1",
        "rollup-plugin-multi-input": "^1.2.0",
        "rollup-plugin-uglify": "^6.0.4"
    }
}
