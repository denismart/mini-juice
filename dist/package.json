{
    "name": "mini-juice",
    "version": "0.8.8",
    "description": "Now you can",
    "author": "denismart",
    "license": "MIT",
    "repository": "denismart/mini-juice",
    "scripts": {
        "build": "yarn prepare-folders && yarn test:lint && yarn test:jest && rm -rf ./dist && rollup --config",
        "build:dev": "rm -rf ./dist && yarn prepare-folders && rollup --config",
        "publish-it": "yarn build && yarn docs && cp package.json ./dist && cp README.md ./dist && git add -f ./dist/package.json && yarn publish dist && cp ./dist/package.json . && git rm --cached ./dist/package.json",
        "test:lint": "eslint .",
        "test:jest": "jest --passWithNoTests",
        "peers-i": "install-peers -f",
        "prepare-folders": "node prepareFolderLists.js",
        "docs": "rm -rf ./docs && jsdoc -r README.md ./src -d ./docs"
    },
    "dependencies": {
        "@vkontakte/vk-bridge": "^2.3.0",
        "axios": "^0.21.1",
        "crypto-js": "^4.0.0",
        "eruda": "^2.4.1",
        "jsdoc": "^3.6.6",
        "konva": "^7.2.2",
        "moment": "^2.29.1",
        "prop-types": "^15.7.2",
        "react-ga": "^3.3.0"
    },
    "peerDependencies": {
        "react": "^16.0.0"
    },
    "devDependencies": {
        "@babel/plugin-external-helpers": "^7.12.1",
        "@babel/plugin-transform-modules-commonjs": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@rollup/plugin-commonjs": "^17.0.0",
        "cross-env": "^7.0.2",
        "eslint": "^5.16.0 || ^6.8.0 || ^7.2.0",
        "eslint-config-airbnb": "18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4 || ^3 || ^2.3.0 || ^1.7.0",
        "install-peers-cli": "^2.2.0",
        "jest": "^26.6.3",
        "prettier": "^2.0.4",
        "prettier-eslint": "^12.0.0",
        "react": "^16.0.0",
        "react-dom": "^16.13.1",
        "react-scripts": "^3.4.1",
        "rollup": "^2.36.1",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-multi-input": "^1.2.0",
        "rollup-plugin-node-resolve": "^5.2.0"
    }
}
